{
    "name": "2zt/blade-cssgg",
    "description": "A package to easily make use of css.gg icons in your Laravel Blade views.",
    "keywords": [
        "Blade",
        "css.gg",
        "icons",
        "Laravel"
    ],
    "homepage": "https://github.com/2zt/blade-cssgg",
    "license": "MIT",
    "authors": [
        {
            "name": "2zt"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "blade-ui-kit/blade-icons": "^1.1",
        "illuminate/support": "^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "BladeUI\\CssggIcons\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BladeUI\\CssggIcons\\BladeCssggIconsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}